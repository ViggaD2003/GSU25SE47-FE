{
  "expo": {
    "name": "MindfulCare",
    "slug": "mindfulcare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/adaptive-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/adaptive-icon.png",
      "backgroundColor": "#ffffff",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png"
      },
      "package": "com.mindfulcare.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./src/assets/adaptive-icon.png"
    },
    "plugins": [
      "expo-notifications",
      [
        "expo-calendar",
        {
          "calendarPermission": "Allow MindfulCare to access your calendar."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "09345b12-72dd-481f-9a08-ecbc1f390238"
      }
    },
    "updates": {
      "enabled": false,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    }
  }
}
